<?xml version="1.0" encoding="utf-8"?>
<mod.remaker.view.SurfaceGroupView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/group_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <mod.remaker.view.DividerRecyclerView
        android:id="@+id/recycler_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:nestedScrollingEnabled="false"
        app:dividerColor="@color/outline_variant_40"
        app:dividerInsetEnd="16dp"
        app:dividerInsetStart="16dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />

</mod.remaker.view.SurfaceGroupView>
